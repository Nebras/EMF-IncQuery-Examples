<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="teachers"
         point="org.eclipse.incquery.runtime.matcherfactory">
      <matcher
            factoryProvider="teachers.TeachersMatcherFactory$Provider"
            id="teachers">
      </matcher>
   </extension>
   <extension
         id="extension.derived.teachers"
         point="org.eclipse.incquery.runtime.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="School"
            feature-name="numberOfTeachers"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="teachersWithMostCourses"
         point="org.eclipse.incquery.runtime.matcherfactory">
      <matcher
            factoryProvider="teacherswithmostcourses.TeachersWithMostCoursesMatcherFactory$Provider"
            id="teachersWithMostCourses">
      </matcher>
   </extension>
   <extension
         id="extension.derived.teachersWithMostCourses"
         point="org.eclipse.incquery.runtime.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="School"
            feature-name="teachersWithMostCourses"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="lastYear"
         point="org.eclipse.incquery.runtime.matcherfactory">
      <matcher
            factoryProvider="lastyear.LastYearMatcherFactory$Provider"
            id="lastYear">
      </matcher>
   </extension>
   <extension
         id="extension.derived.lastYear"
         point="org.eclipse.incquery.runtime.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="School"
            feature-name="lastYear"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="homeroomCourses"
         point="org.eclipse.incquery.runtime.matcherfactory">
      <matcher
            factoryProvider="homeroomcourses.HomeroomCoursesMatcherFactory$Provider"
            id="homeroomCourses">
      </matcher>
   </extension>
   <extension
         id="extension.derived.homeroomCourses"
         point="org.eclipse.incquery.runtime.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="SchoolClass"
            feature-name="homeroomCourses"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="courseWeights"
         point="org.eclipse.incquery.runtime.matcherfactory">
      <matcher
            factoryProvider="courseweights.CourseWeightsMatcherFactory$Provider"
            id="courseWeights">
      </matcher>
   </extension>
   <extension
         id="extension.derived.courseWeights"
         point="org.eclipse.incquery.runtime.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="Year"
            feature-name="weightOfRegularCourses"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>

</plugin>
