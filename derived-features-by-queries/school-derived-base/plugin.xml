<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="extension.derived.teachers"
         name=""
         point="org.eclipse.viatra2.emf.incquery.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="School"
            feature-name="numberOfTeachers"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="extension.derived.teachersWithMostCourses"
         name=""
         point="org.eclipse.viatra2.emf.incquery.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="School"
            feature-name="teachersWithMostCourses"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="extension.derived.lastYear"
         name=""
         point="org.eclipse.viatra2.emf.incquery.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="School"
            feature-name="lastYear"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="extension.derived.homeroomCourses"
         name=""
         point="org.eclipse.viatra2.emf.incquery.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="SchoolClass"
            feature-name="homeroomCourses"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="extension.derived.courseWeights"
         name=""
         point="org.eclipse.viatra2.emf.incquery.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="Year"
            feature-name="weightOfRegularCourses"
            package-nsUri="http:///school-derived.ecore">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="teachers"
         point="org.eclipse.viatra2.emf.incquery.matcherfactory">
      <matcher
            factory="teachers.TeachersMatcherFactory"
            id="teachers">
      </matcher>
   </extension>
   <extension
         id="teachersWithMostCourses"
         point="org.eclipse.viatra2.emf.incquery.matcherfactory">
      <matcher
            factory="teacherswithmostcourses.TeachersWithMostCoursesMatcherFactory"
            id="teachersWithMostCourses">
      </matcher>
   </extension>
   <extension
         id="lastYear"
         point="org.eclipse.viatra2.emf.incquery.matcherfactory">
      <matcher
            factory="lastyear.LastYearMatcherFactory"
            id="lastYear">
      </matcher>
   </extension>
   <extension
         id="homeroomCourses"
         point="org.eclipse.viatra2.emf.incquery.matcherfactory">
      <matcher
            factory="homeroomcourses.HomeroomCoursesMatcherFactory"
            id="homeroomCourses">
      </matcher>
   </extension>
   <extension
         id="courseWeights"
         point="org.eclipse.viatra2.emf.incquery.matcherfactory">
      <matcher
            factory="courseweights.CourseWeightsMatcherFactory"
            id="courseWeights">
      </matcher>
   </extension>

</plugin>
