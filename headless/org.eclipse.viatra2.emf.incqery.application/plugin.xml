<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.viatra2.emf.incqery.application.app"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.viatra2.emf.incqery.application.IncQueryApplication">
         </run>
      </application>
   </extension>
   <extension
         id="incquery"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.viatra2.emf.incqery.application.app"
            name="IncQuery Application">
         <property
               name="appName"
               value="IncQuery Application">
         </property>
      </product>
   </extension>

</plugin>
